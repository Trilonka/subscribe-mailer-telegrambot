{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "docker-compose.yml"
  },
  "deploy": {
    "startCommand": "start.sh $BOT_USERNAME $BOT_TOKEN",
    "healthcheckPath": "/",
    "restartPolicyType": "NEVER",
    "healthcheckTimeout": 100
  }
}
